	<talkaction access="5" log="yes" words="/openserver" script="closeopen.lua"/>
	<talkaction access="5" log="yes" words="/save" filter="first word" script="save.lua"/>

	<!-- Community Manager -->
	<talkaction access="4" log="yes" words="/i" filter="first word" script="createitembyid.lua"/>
